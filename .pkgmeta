package-as: EnchantCheck

license-output: LICENSE

enable-nolib-creation: yes

embedded-libraries:
  - LibStub
  - CallbackHandler-1.0
  - AceAddon-3.0
  - AceConsole-3.0
  - AceDB-3.0
  - AceEvent-3.0
  - AceHook-3.0
  - AceLocale-3.0
  - AceTimer-3.0
  - LibBabble-Inventory-3.0
  - LibItemUpgradeInfo-1.0

ignore:
  - "*.sh"
  - .release
  - .github
  - deprecated
  - README.md
  - CLAUDE.md
  - PUBLISHING.md
  - "*.gif"
  - "*.png"
  - "*.yaml"
  - "*.yml"
  - .gitignore
  - .gitattributes
  - .editorconfig
  - Libs/**/README*
  - Libs/**/CHANGELOG*
  - Libs/**/*.md
  - Libs/**/docs
  - Libs/**/examples
  - Libs/**/tests
