package-as: EnchantCheck

license-output: LICENSE

enable-nolib-creation: yes

ignore:
  - "*.sh"
  - .release
  - .github
  - deprecated
  - README.md
  - CLAUDE.md
  - PUBLISHING.md
  - "*.gif"
  - "*.png"
  - "*.yaml"
  - "*.yml"
  - .gitignore
  - .gitattributes
  - .editorconfig
  - Libs/**/README*
  - Libs/**/CHANGELOG*
  - Libs/**/*.md
  - Libs/**/docs
  - Libs/**/examples
  - Libs/**/tests
